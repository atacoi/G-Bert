#version 330 core

in vec2 TexCoords;

out vec4 FragColor;

uniform sampler2D texture1;

uniform int frameIndex;
uniform float columnCount;
uniform float rowCount;

void main() {
    vec2 tmp = vec2(TexCoords.x / 8.0 + 7.0 / 8.0, TexCoords.y);

    vec4 texColor = texture(texture1, tmp);
    FragColor = texColor;
}